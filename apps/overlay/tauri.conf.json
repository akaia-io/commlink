{
	"$schema": "https://schema.tauri.app/config/2.0.0-rc",
	"productName": "portal",
	"version": "0.1.0",
	"identifier": "akaia.commlink.portal",
	"build": {
		"devUrl": "http://localhost:1420",
		"frontendDist": "../../packages/portal/dist"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"title": "portal",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
